        <instance familyname="Fraunces" filename="instances/Fraunces-LightOpMax.ufo" postscriptfontname="Fraunces-LightOpMax" stylename="Light OpMax">
            <location>
                <dimension name="weight" xvalue="100" />
                <dimension name="wonk" xvalue="0.5" />
                <dimension name="optical" xvalue="144" />
            </location>
            <kerning />
